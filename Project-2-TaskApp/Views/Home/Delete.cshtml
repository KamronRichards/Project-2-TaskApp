@{
    ViewData["Title"] = "Delete Task";
}
@model TaskData

<h2>Confrim Deletion</h2>
<h3>Are you sure you want to delete the task: @Model.taskName?</h3>

<form method="post" asp-controller="Home" asp-action="Delete">
    <button type="submit">Delete</button>
    <a asp-controller="Home" asp-action="Quadrants" class="btn btn-primary">Cancel</a>

    <input type="hidden" asp-for="taskId" />
</form>
